name: atcoder-friends
version: 1.0
author: asi1024
synopsis: atcoder-friends
cabal-version: >= 1.10
build-type: Simple

library
  hs-source-dirs: src
  default-Language: Haskell2010
  build-depends: base >= 4 && < 5
  exposed-modules:
    App
  ghc-options: -Wall -fno-warn-orphans

executable main
  Hs-source-dirs: src
  Default-Language: Haskell2010
  main-is:  Main.hs
  build-depends: base >= 4 && < 5
  ghc-options: -Wall
